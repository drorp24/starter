= form_for @general_profile do |f|
  - if @general_profile.errors.any?
    #error_explanation
      %h2= "#{pluralize(@general_profile.errors.count, "error")} prohibited this general_profile from being saved:"
      %ul
        - @general_profile.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :user_id
    = f.number_field :user_id
  .field
    = f.label :title
    = f.text_field :title
  .actions
    = f.submit 'Save'
