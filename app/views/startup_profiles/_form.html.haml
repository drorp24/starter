= form_for @startup_profile do |f|
  - if @startup_profile.errors.any?
    #error_explanation
      %h2= "#{pluralize(@startup_profile.errors.count, "error")} prohibited this startup_profile from being saved:"
      %ul
        - @startup_profile.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :role
    = f.text_field :role
  .actions
    = f.submit 'Save'
